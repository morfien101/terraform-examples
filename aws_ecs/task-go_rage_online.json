[
  {
    "cpu": 256,
    "essential": true,
    "image": "${image_url}",
    "memory": 512,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": 8001
      }
    ],
    "environment": [
      {"name":"SPECIAL_DATA", "value":"TheRandyOne"}
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}"
        }
    }
  }
]
